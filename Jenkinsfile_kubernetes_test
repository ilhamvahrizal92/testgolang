pipeline {
    agent any
    stages {
        stage ('build') {
            steps {
                echo "Build App"
                echo 'docker build -t asia-southeast2-docker.pkg.dev/tugas-devops/go-app:1 .'
            }
        }
        stage ('Push to GCR') {
            steps {
                echo "push to gcr"
                echo 'docker push asia-southeast2-docker.pkg.dev/tugas-devops/go-app:1'
            }
        }
    }
}
